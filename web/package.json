    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "axios": "^1.2.1",
    "core-js": "^3.8.3",
    "element-plus": "^2.2.27",
    "jwt-decode": "^3.1.2",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mitt": "^3.0.0",
    "swiper": "^8.4.5",
